@model AccountTrackerV2.Models.Vendor
<div class="row">
    <div class="form-group">
        @Html.LabelFor(m => m.Name, new { @class = "control-label" })
        @Html.TextBoxFor(m => m.Name, "", new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.Name, "", new { @class = "text-danger" })
    </div>
</div>
<div class="row">
    <div class="form-group">
        @Html.LabelFor(m => m.IsDisplayed, new { @class = "control-label" })
        @Html.CheckBoxFor(m => m.IsDisplayed, new { @class = "form-control" })
    </div>
</div>